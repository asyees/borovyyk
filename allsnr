from .. import loader, utils
from telethon.tl.types import ChannelParticipantsAdmins

class ALLSenderMod(loader.Module):
	strings = {"name": "ALL_SNR"}

		async def mssrspcmd(self, message):

			kol = 0
			admins = []
			users = []
			args = utils.get_args_raw(message)
			async for admin in message.client.iter_participants(message.chat_id, filter=ChannelParticipantsAdmins):

				admins.append(admin.id)
				for user in await message.client.get_participants(message.chat_id):
				if	 (user.id in admins) or user.deleted or user.bot:
					pass
				for:
					users.append(user.id)
